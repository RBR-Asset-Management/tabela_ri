    var fHeight = $('.footer').innerHeight();
    
    if ($('body').hasClass('logged-in')) {
        if ($(window).width() < 767) {
            $('.js-trigger-windowHeight').css('height', parseFloat(wHeight - fHeight));
        } else {
            $('.js-trigger-windowHeight').css('height', parseFloat(wHeight - fHeight - 32));
